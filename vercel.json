{
  "version": 1,
  "builds": [
    {
      "src": "./src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "src/routes/index.ts",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "PATCH",
        "DELETE",
        "PUT"
      ]
    }
  ]
}